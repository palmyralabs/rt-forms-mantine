  },
  "peerDependencies": {
    "@palmyralabs/palmyra-wire": "^1.1.0",
    "@palmyralabs/rt-forms": "github:palmyralabs/rt-forms",
    "@palmyralabs/ts-predicates": "github:palmyralabs/ts-predicates",
