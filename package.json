  "devDependencies": {
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.7.6",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "glob": "^11.0.0",
    "jsdom": "^25.0.1",
    "serve-static": "^1.16.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "usehooks-ts": "^3.1.0",
    "vite": "^5.4.9",
    "vite-plugin-dts": "^4.2.4",
