{
  "name": "@palmyralabs/rt-forms-mantine",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js"
    }
  },
  "files": [
    "dist"
